package code {		import gs.TweenLite;	import at.ac.tuwien.igw.tuio.*;	import flash.events.*;	import flash.display.*;	import flash.system.*;		public class Main extends Sprite{			var floscCont:Sprite = new Sprite();		var toucheCont:Sprite = new Sprite();		var bg:Sprite = new Sprite();			public function Main() {				Security.allowDomain('*');					bg.graphics.beginFill(0xeeeeee, 0);			bg.graphics.drawRect(0, 0, stage.stageWidth, stage.stageHeight);			bg.addEventListener(TouchEventMod.MOUSE_DOWN, floscTouch);			bg.addEventListener(TouchEvent.MOUSE_DOWN, toucheTouch);			//bg.addEventListener(MouseEvent.MOUSE_DOWN, mTouch);			bg.name = "bg";									floscCont.graphics.beginFill(0xeeeeee, 1);			floscCont.graphics.drawRect(0, 0, stage.stageWidth, stage.stageHeight);			floscCont.name = "flosc";						toucheCont.graphics.beginFill(0x99ff99, 0);			toucheCont.graphics.drawRect(0, 0, stage.stageWidth, stage.stageHeight);			toucheCont.name = "touche";						stage.addChild(floscCont);			stage.addChild(toucheCont);			stage.addChild(bg);						TUIO.init(stage, false);			TUIOMod.init(stage, "localhost", 3000, "", false);						//bg.addEventListener(TouchEventMod.MOUSE_MOVE, floscTouch);			//bg.addEventListener(TouchEvent.MOUSE_MOVE, toucheTouch);						//bg.addEventListener(TouchEventMod.MOUSE_UP, floscTouch);			//bg.addEventListener(TouchEvent.MOUSE_UP, toucheTouch);				}				function floscTouch(e:TouchEventMod) {			var r:Ring = new Ring(floscCont, e.stageX, e.stageY, 30, 0xFF6600);			r.addEventListener(TouchEventMod.MOUSE_MOVE, r.moveMe);			r.addEventListener(TouchEventMod.MOUSE_UP, r.fadeOut);			TUIOMod.addObjectListener(e.ID, r);		}				function toucheTouch(e:TouchEvent) {			var r:Ring = new Ring(toucheCont, e.stageX, e.stageY, 30, 0x0066FF);			r.addEventListener(TouchEvent.MOUSE_MOVE, r.moveMe);			r.addEventListener(TouchEvent.MOUSE_UP, r.fadeOut);			TUIO.addObjectListener(e.ID, r);		}			}	}